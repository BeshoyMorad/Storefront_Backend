{
  "name": "storefront_backend",
  "version": "1.0.0",
  "description": "## Getting Started",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "npm run build && nodemon src/server.ts",
    "prod": "npm run build && nodemon dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"nodemon ./dist/server.js\"",
    "up:test": "db-migrate --env test up",
    "down:test": "db-migrate --env test down -c 4",
    "up:dev": "db-migrate up",
    "down:dev": "db-migrate down -c 4",
    "test": "set ENV=test && npm run build && npm run up:test && jasmine && npm run down:test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BeshoyMorad/Storefront_Backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BeshoyMorad/Storefront_Backend/issues"
  },
  "homepage": "https://github.com/BeshoyMorad/Storefront_Backend#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.16",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0"
  }
}
